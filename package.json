{
  "name": "vue3-basic",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --host",
    "mock": "vite --mode mock --host",
    "staging": "vite --mode production --host",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "lint": "pnpm lint:stylelint && pnpm lint:prettier && pnpm lint:eslint",
    "lint:lint-staged": "lint-staged -c .husky/lintstagedrc.js",
    "lint:stylelint": "stylelint **/*.{css,scss,vue,html} --fix --ignore-path .gitignore",
    "lint:prettier": "prettier --write  \"./**/*.{js,ts,json,json5,css,scss,vue,html,md}\" --ignore-path .gitignore",
    "lint:eslint": "eslint  \"./**/{\\.*,*}.{vue,js,ts}\" --fix --ignore-path .gitignore",
    "prepare": "husky install"
  },
  "dependencies": {
    "@amap/amap-jsapi-loader": "^1.0.1",
    "@codemirror/lang-javascript": "^6.1.9",
    "@element-plus/icons-vue": "^2.1.0",
    "@types/dplayer": "^1.25.2",
    "@types/marked": "^5.0.1",
    "@vueuse/core": "^9.6.0",
    "@vueuse/motion": "^2.0.0",
    "@zxing/library": "^0.19.1",
    "axios": "^1.1.3",
    "codemirror": "^6.0.1",
    "dayjs": "^1.11.7",
    "dplayer": "^1.27.1",
    "echarts": "^5.4.1",
    "element-plus": "^2.2.19",
    "highlight.js": "^11.8.0",
    "lodash-es": "^4.17.21",
    "marked": "^5.1.1",
    "marked-highlight": "^2.0.1",
    "pdfjs-dist": "^3.8.162",
    "pinia": "^2.0.23",
    "socket.io-client": "^4.6.1",
    "three": "^0.153.0",
    "ts-md5": "^1.3.1",
    "vue": "^3.2.41",
    "vue-countup-v3": "^1.3.0",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "@types/express": "^4.17.14",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.11.8",
    "@types/three": "^0.152.1",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-vue": "^9.7.0",
    "express": "^4.18.2",
    "husky": "^8.0.1",
    "json5": "^2.2.1",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.19",
    "postcss-html": "^1.5.0",
    "prettier": "^2.7.1",
    "sass": "^1.55.0",
    "socket.io": "^4.6.1",
    "stylelint": "^14.16.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-standard-scss": "^6.1.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "typescript": "^4.6.4",
    "vite": "^3.2.0",
    "vite-plugin-mock": "2.9.6",
    "vite-plugin-windicss": "^1.8.10",
    "vite-svg-loader": "^4.0.0",
    "vue-tsc": "^1.0.9",
